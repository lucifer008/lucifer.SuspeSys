<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <system.runtime.remoting>
    <application name="SuspeSys.Remoting">
      <service>
        <wellknown mode="SingleCall" type="SuspeSys.Service.Impl.CatService,SuspeSys.Service.Impl" objectUri="ChatRoomURL"></wellknown>
      </service>
      <channels>
        <!---xml 配置方式，支持http和tcp两种，但是每个接口得手动配置-->
        <channel ref="tcp" port="30001">
          <serverProviders>
            <provider ref="wsdl"></provider>
            <formatter ref="binary" typeFilterLevel="Full"></formatter>
          </serverProviders>
          <clientProviders>
            <formatter ref="binary"></formatter>
          </clientProviders>
        </channel>
      </channels>
    </application>
  </system.runtime.remoting>
</configuration>