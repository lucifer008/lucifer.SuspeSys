<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ProcessFlowVersion" table="ProcessFlowVersion" lazy="false" >
    <id name="Id" column="Id">
      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <many-to-one name="ProcessOrder">
      <column name="PROCESSORDER_Id" sql-type="char" not-null="false" />
    </many-to-one>
    <property name="ProVersionNum">
      <column name="ProVersionNum" sql-type="varchar" not-null="false" />
    </property>
    <property name="ProVersionNo">
      <column name="ProVersionNo" sql-type="char" not-null="false" />
    </property>
    <property name="ProcessVersionName">
      <column name="ProcessVersionName" sql-type="char" not-null="false" />
    </property>
    <property name="EffectiveDate">
      <column name="EffectiveDate" sql-type="datetime" not-null="false" />
    </property>
    <property name="TotalStandardPrice">
      <column name="TotalStandardPrice" sql-type="char" not-null="false" />
    </property>
    <property name="TotalSam">
      <column name="TotalSAM" sql-type="char" not-null="false" />
    </property>
    <property name="UpdateDateTime">
      <column name="UpdateDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="InsertDate">
      <column name="InsertDate" sql-type="datetime" not-null="false" />
    </property>
    <property name="InsertUser">
      <column name="InsertUser" sql-type="char" not-null="false" />
    </property>
    <property name="UpdateUser">
      <column name="UpdateUser" sql-type="char" not-null="false" />
    </property>
    <property name="Deleted">
      <column name="Deleted" sql-type="tinyint" not-null="false" />
    </property>
    <property name="CompanyId">
      <column name="CompanyId" sql-type="char" not-null="false" />
    </property>
  </class>
</hibernate-mapping>