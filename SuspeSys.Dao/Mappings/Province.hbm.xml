<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Province" table="Province" lazy="false" >
    <id name="Id" column="Id">
      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <property name="ProvinceCode">
      <column name="ProvinceCode" sql-type="varchar" not-null="false" />
    </property>
    <property name="ProvinceName">
      <column name="ProvinceName" sql-type="varchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>