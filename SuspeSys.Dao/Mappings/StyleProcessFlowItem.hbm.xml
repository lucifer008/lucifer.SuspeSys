<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="StyleProcessFlowItem" table="StyleProcessFlowItem" lazy="false" >
    <id name="Id" column="Id">
      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <many-to-one name="BasicProcessFlow">
      <column name="BASICPROCESSFLOW_Id" sql-type="char" not-null="false" />
    </many-to-one>
    <many-to-one name="Style">
      <column name="STYLE_Id" sql-type="char" not-null="false" />
    </many-to-one>
    <property name="ProVersionNo">
      <column name="ProVersionNo" sql-type="char" not-null="false" />
    </property>
    <property name="ProcessVersionName">
      <column name="ProcessVersionName" sql-type="char" not-null="false" />
    </property>
    <property name="FlowNo">
      <column name="FlowNo" sql-type="varchar" not-null="false" />
    </property>
    <property name="ProcessCode">
      <column name="ProcessCode" sql-type="char" not-null="false" />
    </property>
    <property name="ProcessStatus">
      <column name="ProcessStatus" sql-type="tinyint" not-null="false" />
    </property>
    <property name="Sam">
      <column name="SAM" sql-type="char" not-null="false" />
    </property>
    <property name="StanardHours">
      <column name="StanardHours" sql-type="char" not-null="false" />
    </property>
    <property name="StandardPrice">
      <column name="StandardPrice" sql-type="decimal" not-null="false" />
    </property>
    <property name="InsertDateTime">
      <column name="InsertDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="UpdateDateTime">
      <column name="UpdateDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="InsertUser">
      <column name="InsertUser" sql-type="char" not-null="false" />
    </property>
    <property name="UpdateUser">
      <column name="UpdateUser" sql-type="char" not-null="false" />
    </property>
    <property name="Deleted">
      <column name="Deleted" sql-type="tinyint" not-null="false" />
    </property>
    <property name="CompanyId">
      <column name="CompanyId" sql-type="char" not-null="false" />
    </property>
  </class>
</hibernate-mapping>