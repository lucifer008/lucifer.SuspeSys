<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="UserClientMachines" table="UserClientMachines" lazy="false" >
    <id name="Id" column="Id">
      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <property name="UserId">
      <column name="UserId" sql-type="char" not-null="true" />
    </property>
    <property name="ClientMachineId">
      <column name="ClientMachineId" sql-type="char" not-null="true" />
    </property>
  </class>
</hibernate-mapping>