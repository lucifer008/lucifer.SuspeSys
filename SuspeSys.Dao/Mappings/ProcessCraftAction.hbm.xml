    </property>
    <property name="ProSectionNo">
      <column name="ProSectionNo" sql-type="char" not-null="false" />
    </property>
    <property name="ProSectionCode">
      <column name="ProSectionCode" sql-type="char" not-null="false" />
    </property>
    <property name="ProSectionName">
      <column name="ProSectionName" sql-type="char" not-null="false" />
    </property>
    <property name="Remark2">
      <column name="Remark2" sql-type="char" not-null="false" />
    </property>
    <property name="Deleted">
      <column name="Deleted" sql-type="tinyint" not-null="false" />
    </property>
    <property name="CompanyId">
      <column name="CompanyId" sql-type="char" not-null="false" />
    </property>
  </class>
</hibernate-mapping>