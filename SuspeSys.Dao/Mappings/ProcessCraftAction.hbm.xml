      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <property name="ActionNo">
      <column name="ActionNo" sql-type="char" not-null="false" />
    </property>
    <property name="ActionDesc">
      <column name="ActionDesc" sql-type="char" not-null="false" />
    </property>
    <property name="IsEnabled">
