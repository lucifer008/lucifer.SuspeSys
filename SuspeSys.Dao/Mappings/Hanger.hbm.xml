<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Hanger" table="Hanger" lazy="false" >
    <id name="Id" column="Id">
      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <property name="HangerNo">
      <column name="HangerNo" sql-type="bigint" not-null="false" />
    </property>
    <property name="RegisterDate">
      <column name="RegisterDate" sql-type="datetime" not-null="false" />
    </property>
  </class>
</hibernate-mapping>