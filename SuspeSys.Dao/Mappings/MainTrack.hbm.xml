<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="MainTrack" table="MainTrack" lazy="false" >
    <id name="Id" column="Id">
      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <property name="GroupNo">
      <column name="GroupNo" sql-type="varchar" not-null="false" />
    </property>
    <property name="Num">
      <column name="Num" sql-type="smallint" not-null="false" />
    </property>
    <property name="Status">
      <column name="Status" sql-type="tinyint" not-null="false" />
    </property>
    <property name="StartDateTime">
      <column name="StartDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="EmergencyStopDateTime">
      <column name="EmergencyStopDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="StopDateTime">
      <column name="StopDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="Memo">
      <column name="Memo" sql-type="varchar" not-null="false" />
    </property>
    <property name="InsertDateTime">
      <column name="InsertDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="UpdateDateTime">
      <column name="UpdateDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="InsertUser">
      <column name="InsertUser" sql-type="char" not-null="false" />
    </property>
    <property name="UpdateUser">
      <column name="UpdateUser" sql-type="char" not-null="false" />
    </property>
    <property name="Deleted">
      <column name="Deleted" sql-type="tinyint" not-null="false" />
    </property>
    <property name="CompanyId">
      <column name="CompanyId" sql-type="char" not-null="false" />
    </property>
  </class>
</hibernate-mapping>