<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="EmployeeRoleRelation" table="EmployeeRoleRelation" lazy="false" >
    <id name="Id" column="Id">
      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <property name="RolesId">
      <column name="ROLES_Id" sql-type="char" not-null="false" />
    </property>
    <property name="EmployeeId">
      <column name="EMPLOYEE_Id" sql-type="char" not-null="false" />
    </property>
  </class>
</hibernate-mapping>