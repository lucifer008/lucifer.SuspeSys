<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Modules" table="Modules" lazy="false" >
    <id name="Id" column="Id">
      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <many-to-one name="ModulesVal">
      <column name="MODULES_Id" sql-type="char" not-null="false" />
    </many-to-one>
    <property name="ActionName">
      <column name="ActionName" sql-type="varchar" not-null="true" />
    </property>
    <property name="ActionKey">
      <column name="ActionKey" sql-type="varchar" not-null="true" />
    </property>
    <property name="Description">
      <column name="Description" sql-type="varchar" not-null="false" />
    </property>
    <property name="ModulesType">
      <column name="ModulesType" sql-type="int" not-null="true" />
    </property>
    <property name="ModuleLevel">
      <column name="ModuleLevel" sql-type="int" not-null="true" />
    </property>
    <property name="OrderField">
      <column name="OrderField" sql-type="decimal" not-null="true" />
    </property>
    <property name="InsertDateTime">
      <column name="InsertDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="InsertUser">
      <column name="InsertUser" sql-type="char" not-null="false" />
    </property>
    <property name="UpdateDateTime">
      <column name="UpdateDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="UpdateUser">
      <column name="UpdateUser" sql-type="char" not-null="false" />
    </property>
    <property name="Deleted">
      <column name="Deleted" sql-type="tinyint" not-null="false" />
    </property>
    <property name="CompanyId">
      <column name="CompanyId" sql-type="char" not-null="false" />
    </property>
  </class>
</hibernate-mapping>