<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="Area" table="Area" lazy="false" >
    <id name="Id" column="Id">
      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <many-to-one name="City">
      <column name="CITY_Id" sql-type="char" not-null="false" />
    </many-to-one>
    <property name="AreaName">
      <column name="AreaName" sql-type="varchar" not-null="false" />
    </property>
    <property name="AreaCode">
      <column name="AreaCode" sql-type="char" not-null="false" />
    </property>
    <property name="Addess">
      <column name="Addess" sql-type="varchar" not-null="false" />
    </property>
    <property name="Memo">
      <column name="Memo" sql-type="varchar" not-null="false" />
    </property>
  </class>
</hibernate-mapping>