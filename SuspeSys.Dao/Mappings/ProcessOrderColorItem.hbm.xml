<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="ProcessOrderColorItem" table="ProcessOrderColorItem" lazy="false" >
    <id name="Id" column="Id">
      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <many-to-one name="ProcessOrder">
      <column name="PROCESSORDER_Id" sql-type="char" not-null="false" />
    </many-to-one>
    <many-to-one name="PoColor">
      <column name="POCOLOR_Id" sql-type="char" not-null="false" />
    </many-to-one>
    <many-to-one name="CustomerPurchaseOrder">
      <column name="CUSTOMERPURCHASEORDER_Id" sql-type="char" not-null="false" />
    </many-to-one>
    <property name="MOrderItemNo">
      <column name="MOrderItemNo" sql-type="varchar" not-null="false" />
    </property>
    <property name="Color">
      <column name="Color" sql-type="varchar" not-null="false" />
    </property>
    <property name="ColorDescription">
      <column name="ColorDescription" sql-type="varchar" not-null="false" />
    </property>
    <property name="Total">
      <column name="Total" sql-type="int" not-null="false" />
    </property>
    <property name="InsertDateTime">
      <column name="InsertDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="UpdateDateTime">
      <column name="UpdateDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="InsertUser">
      <column name="InsertUser" sql-type="char" not-null="false" />
    </property>
    <property name="UpdateUser">
      <column name="UpdateUser" sql-type="char" not-null="false" />
    </property>
    <property name="Deleted">
      <column name="Deleted" sql-type="tinyint" not-null="false" />
    </property>
    <property name="CompanyId">
      <column name="CompanyId" sql-type="char" not-null="false" />
    </property>
  </class>
</hibernate-mapping>