<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="CustomerPurchaseOrder" table="CustomerPurchaseOrder" lazy="false" >
    <id name="Id" column="Id">
      <column name="Id" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <many-to-one name="Customer">
      <column name="CUSTOMER_Id" sql-type="char" not-null="false" />
    </many-to-one>
    <many-to-one name="Style">
      <column name="STYLE_Id" sql-type="char" not-null="false" />
    </many-to-one>
    <property name="CusNo">
      <column name="CusNo" sql-type="varchar" not-null="false" />
    </property>
    <property name="CusName">
      <column name="CusName" sql-type="varchar" not-null="false" />
    </property>
    <property name="CusPurOrderNum">
      <column name="CusPurOrderNum" sql-type="bigint" not-null="false" />
    </property>
    <property name="PurchaseOrderNo">
      <column name="PurchaseOrderNo" sql-type="varchar" not-null="false" />
    </property>
    <property name="OrderNo">
      <column name="OrderNo" sql-type="varchar" not-null="false" />
    </property>
    <property name="GeneratorDate">
      <column name="GeneratorDate" sql-type="datetime" not-null="false" />
    </property>
    <property name="DeliveryDate">
      <column name="DeliveryDate" sql-type="datetime" not-null="false" />
    </property>
    <property name="Mobile">
      <column name="Mobile" sql-type="varchar" not-null="false" />
    </property>
    <property name="DeliverAddress">
      <column name="DeliverAddress" sql-type="varchar" not-null="false" />
    </property>
    <property name="Address">
      <column name="Address" sql-type="varchar" not-null="false" />
    </property>
    <property name="LinkMan">
      <column name="LinkMan" sql-type="varchar" not-null="false" />
    </property>
    <property name="Tel">
      <column name="Tel" sql-type="varchar" not-null="false" />
    </property>
    <property name="InsertDateTime">
      <column name="InsertDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="UpdateDateTime">
      <column name="UpdateDateTime" sql-type="datetime" not-null="false" />
    </property>
    <property name="InsertUser">
      <column name="InsertUser" sql-type="char" not-null="false" />
    </property>
    <property name="UpdateUser">
      <column name="UpdateUser" sql-type="char" not-null="false" />
    </property>
    <property name="Deleted">
      <column name="Deleted" sql-type="tinyint" not-null="false" />
    </property>
    <property name="CompanyId">
      <column name="CompanyId" sql-type="char" not-null="false" />
    </property>
  </class>
</hibernate-mapping>