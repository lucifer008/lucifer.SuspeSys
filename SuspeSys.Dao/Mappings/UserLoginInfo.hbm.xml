<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="SuspeSys.Domain" namespace="SuspeSys.Domain" xmlns="urn:nhibernate-mapping-2.2">
  <class name="UserLoginInfo" table="UserLoginInfo" lazy="false" >
    <id name="SessionId" column="SessionId">
      <column name="SessionId" sql-type="char" not-null="true" />
      <generator class="uuid.hex" />
    </id>
    <property name="UserId">
      <column name="UserId" sql-type="char" not-null="true" />
    </property>
    <property name="UserName">
      <column name="UserName" sql-type="varchar" not-null="true" />
    </property>
    <property name="EmployeeId">
      <column name="EmployeeId" sql-type="char" not-null="true" />
    </property>
    <property name="EmployeeName">
      <column name="EmployeeName" sql-type="varchar" not-null="true" />
    </property>
    <property name="LoginDate">
      <column name="LoginDate" sql-type="datetime" not-null="true" />
    </property>
    <property name="LoginOutDate">
      <column name="LoginOutDate" sql-type="datetime" not-null="false" />
    </property>
  </class>
</hibernate-mapping>