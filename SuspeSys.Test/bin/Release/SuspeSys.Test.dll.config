<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="hibernate-configuration" type="NHibernate.Cfg.ConfigurationSectionHandler, NHibernate" />
  </configSections>
  <appSettings>
    <add key="SusTcpPort" value="9999" />
    <add key="CANPort" value="9998" />
    <add key="CANIp" value="127.0.0.1" />
    <add key="RedisIp" value="127.0.0.1" />
    <add key="RedisPort" value="6379" />
    <!--<add key="CANPort" value="6001"/>
    <add key="CANIp" value="192.168.1.07"/>-->
    <add key="CompanyId" value="1631de7e-fed2-4da2-8e3a-6135be5c040b" />
  </appSettings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6" />
  </startup>

  <hibernate-configuration xmlns="urn:nhibernate-configuration-2.2">
    <session-factory name="NHibernate.Test">
      <property name="show_sql">true</property>
      <property name="hbm2ddl.auto">update</property>
      <property name="current_session_context_class">thread_static</property>
      
      <property name="connection.driver_class">NHibernate.Driver.SqlClientDriver</property>
      <property name="connection.connection_string">
        Data Source=.;Initial Catalog=SyspeSysV2.0;Persist Security Info=True;User ID=sa;Password=sa
      </property>
      <property name="dialect">NHibernate.Dialect.MsSql2008Dialect</property>
      <mapping assembly="SuspeSys.Dao" />
    </session-factory>
  </hibernate-configuration>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
